{
  "name": "prepackage-checks",
  "description": "Validates package invariants before publishing",
  "author": "Andrew Aylett",
  "version": "0.1.2",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewaylett/self-throttle.git"
  },
  "type": "module",
  "bin": "./dist/cli.js",
  "scripts": {
    "build": "tsc --build .",
    "postbuild": "node --experimental-vm-modules dist/cli.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests test",
    "lint": "eslint .",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "dev": "npm run lint:fix && npm run build"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "extend-expect": "^1.0.2",
    "glob": "^8.0.3",
    "jest": "^29.0.3"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/glob": "8.1.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.1",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint": "8.34.0",
    "eslint-config-andrewaylett": "2.1.2",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  }
}
